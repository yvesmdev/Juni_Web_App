                            <div class="card">
                                <div class="card-header">
                                    <strong class="card-title">Ventes - Payées</strong>
                                </div>
                                <div class="card-body">
                                    <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                        <thead>
                                            <tr>
                                                <th>Commande</th>
                                                <th>Client</th>
                                                <th>Qté</th>
                                                <th>Total</th>
                                                <th>Payment</th>
                                                <!--<th>Status</th>-->
                                                <th>Operation</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{
                                                int i = 0;
                                                foreach (Order CurOrder in ViewBag.OrderList)
                                                {
                                                    <tr>
                                                        <td>@(CurOrder.OrderUniqueId)</td>
                                                        <td>@(CurOrder.ClientId)</td>
                                                        <td>@(CurOrder.Products.Count)</td>
                                                        <td>@("$" + Order.GetOrderTotal(CurOrder))</td>
                                                        <td>@(Order.OrderTypeMessage(CurOrder.OrderType))</td>
                                                        <td>
                                                            <button class="btn btn-primary" id="see_button_@(++i)" data-toggle="collapse" product-names='@Html.Raw(CurOrder.GetJSonProductNames())' product-prices='@Html.Raw(CurOrder.GetJSonProductPrices())' product-qties='@Html.Raw(CurOrder.GetJSonProductQties())' data-target="#orderPanel" onclick="openProductModal('@(i)',@(CurOrder.DeliveryFee),'@(CurOrder.OrderUniqueId)','@(CurOrder.OrderDate)','@(CurOrder.OrderCompleted)','@(Order.OrderTypeMessage(CurOrder.OrderType))','@(CurOrder.ClientId)','@(CurOrder.SenderFullname)','@(CurOrder.SenderCell)','@(CurOrder.SenderFullname)','@(CurOrder.SenderCell)','@(CurOrder.DestinatorFullname)','@(CurOrder.DestinatorCell)','@(CurOrder.Address)','@(CurOrder.GiftMessage)')">Voir</button>
                                                            <!-- <button class="btn btn-danger">Archiver</button>-->
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>


                    </div>
                </div><!-- .animated -->
            </div><!-- .content -->
